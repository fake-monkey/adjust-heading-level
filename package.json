{
  "name": "structure-aware-markdown-headings",
  "displayName": "Structure-Aware Markdown Headings",
  "description": "Intelligent Markdown heading adjustment using document structure analysis.",
  "publisher": "false-ape",
  "repository": {
    "url": "https://github.com/fake-monkey/adjust-heading-level.git"
  },
  "license": "Apache-2.0",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.62.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.increaseHeading",
        "title": "Increase Heading Level",
        "category": "Structure-Aware Markdown Headings"
      },
      {
        "command": "extension.decreaseHeading",
        "title": "Decrease Heading Level",
        "category": "Structure-Aware Markdown Headings"
      },
      {
        "command": "extension.batchAdjustHeading",
        "title": "Batch Adjust Heading Level",
        "category": "Structure-Aware Markdown Headings"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.62.0",
    "typescript": "^4.5.2"
  }
}
